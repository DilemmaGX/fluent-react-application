{
  "name": "fluent-react-application",
  "keywords": [
    "fluentui",
    "typescript",
    "electron",
    "react",
    "template",
    "application"
  ],
  "version": "1.0.0",
  "homepage": "./",
  "main": "main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "node build.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:dev": "node build.js && electron .",
    "electron": "npm run build && electron .",
    "electron:exe": "npm run build && electron-packager ./ \"Example\" --out ./Application --app-version 1.0.0 --overwrite --asar --prune --ignore=node_modules && powershell -NoProfile -NonInteractive -Command \"Compress-Archive -Path 'Application\\Example-win32-x64' -DestinationPath 'Application\\Example-win32-x64.zip' -Force\""
  },
  "dependencies": {
    "@fluentui/react-components": "^9.54.1",
    "@fluentui/react-icons": "^2.0.242",
    "@primer/octicons-react": "^19.9.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.98",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "electron-packager": "^17.1.2",
    "esbuild": "^0.21.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^31.0.1",
    "esbuild-plugin-inline-image": "^0.0.9"
  }
}
